{
  "name": "SwifterSwiftSSH",
  "version": "0.0.1",
  "summary": "A swift ssh client with libssh",
  "description": "A swift ssh client with libssh",
  "homepage": "https://github.com/ridenui/swifter-swift-ssh",
  "license": "MIT",
  "authors": {
    "Nils Bergmann": "nilsbergmann@noim.io"
  },
  "social_media_url": "https://twitter.com/EpicNilo",
  "platforms": {
    "osx": "10.15.0",
    "ios": "13.0"
  },
  "source": {
    "git": "."
  },
  "source_files": [
    "SwifterSwiftSSH",
    "SwifterSwiftSSH/**/*.{h,m,swift}"
  ],
  "public_header_files": "SwifterSwiftSSH/**/*.h",
  "preserve_paths": [
    "Libraries/lib/**/*.a",
    "Libraries-iOS/lib/**/*.a"
  ],
  "frameworks": "CFNetwork",
  "requires_arc": true,
  "ios": {
    "vendored_libraries": [
      "Libraries-iOS/lib/libssh.a",
      "Libraries-iOS/lib/libssl.a"
    ],
    "libraries": [
      "ssh",
      "ssl"
    ],
    "source_files": [
      "Libraries-iOS",
      "Libraries-iOS/**/*.h"
    ],
    "public_header_files": "Libraries-iOS/**/*.h"
  },
  "osx": {
    "vendored_libraries": [
      "Libraries/lib/libssh.a",
      "Libraries/lib/libssl.a"
    ],
    "libraries": [
      "ssh",
      "ssl"
    ],
    "source_files": [
      "Libraries",
      "Libraries/**/*.h"
    ],
    "public_header_files": "Libraries/**/*.h"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "LIBRARY_SEARCH_PATHS": ""
  },
  "header_mappings_dir": "Libraries/include"
}
